<% if logged_in? %>
  <div class="row">
  <%=render 'static_pages/user_logged_in' %>
    <div class="col-md-8">
      <h3>Micropost Feed</h3>
      <%= render 'shared/feed' %>
    </div>
     <section class="stats">
        <%= render 'shared/stats' %>
      </section>
  </div>
<% else %>
  <%=render 'static_pages/user_not_logged_in' %>
<% end %>
